autoload -U colors; colors
set prompt_subst
export LANG=ja_JP.eucJP
setopt auto_cd
export CLICOLOR=true
alias la='ls -a'
alias a='./a.out'

setopt auto_list
setopt auto_param_keys
setopt list_packed
setopt list_types
unsetopt list_beep
setopt rec_exact
setopt interactive_comments
setopt magic_equal_subst
setopt complete_in_word
setopt always_last_prompt
